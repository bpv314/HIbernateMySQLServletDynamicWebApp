<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.belen.registration.bean.Member" table="member">
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="firstName" column="first_name" type="java.lang.String" length="30" />
		<property name="lastName" column="last_name" type="java.lang.String"  length="30"/>
		<property name="username" column="username" type="java.lang.String" length="30" />
		<property name="password" column="password" type="java.lang.String" length="30"/>
		<property name="city" column="city" type="java.lang.String" length="30"/>
	</class>
</hibernate-mapping>